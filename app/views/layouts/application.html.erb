        <li><%= link_to "HELP", help_path %></li>
      </ul>
      
    </header>
    
    <div class="container">
    
    
    
    <%= yield %>
    
    </div>
    
    <footer class="footer">
      <h1>Look I'm at the bottom...</h1>
    </footer>
    
    
  </body>
</html>
